<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    td><%= client.movies.each do |movie| %>
      <span><%= movie.name %> </span>
      <% end %>
    </td>
      </td>
    <td><%= link_to "Mostrar cliente", client, style: "text-decoration: none;" %></td>
    <td><%= link_to "Editar cliente", edit_client_path(client), style: "text-decoration: none;" %></td>
    <td><%= button_to "Borrar cliente", @client, method: :delete, class: "btn btn-outline-primary" %></td>
  </tr>
</div>